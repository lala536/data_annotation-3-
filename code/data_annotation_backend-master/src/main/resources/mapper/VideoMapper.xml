<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kevin.data_annotation_backendmaster.mapper.VideoMapper">
    <select id="getAllVideo" resultType="com.kevin.data_annotation_backendmaster.entity.Video">
        select * from video
    </select>

    <select id="getMember" resultType="com.kevin.data_annotation_backendmaster.entity.Video">
        select * from video where class_id = #{classId} and group_id = #{groupId}
    </select>
</mapper>